<file path="tg-intel/apps/frontend/components/channels/columns.tsx">
"use client";

import { ColumnDef } from "@tanstack/react-table";
import { Button } from "../ui/button";
import { ArrowUpDownIcon } from "../icons";

export type ChannelPost = {
  id: number;
  tg_message_id: number;
  posted_at?: string | null;
  text?: string | null;
  views?: number | null;
  forwards?: number | null;
  replies?: number | null;
  reactions?: number | null;
};

export const columns: ColumnDef<ChannelPost>[] = [
  {
    accessorKey: "tg_message_id",
    header: ({ column }: { column: any }) => (
      <Button variant="ghost" onClick={() => column.toggleSorting(column.getIsSorted() === "asc")}>
        Msg #<ArrowUpDownIcon className="ml-1 inline" />
      </Button>
    ),
    cell: ({ row }: { row: any }) => <span>#{row.getValue("tg_message_id")}</span>,
  },
  {
    accessorKey: "posted_at",
    header: ({ column }: { column: any }) => (
      <Button variant="ghost" onClick={() => column.toggleSorting(column.getIsSorted() === "asc")}> 
        Posted<ArrowUpDownIcon className="ml-1 inline" />
      </Button>
    ),
  },
  {
    accessorKey: "text",
